src = ['main.c', 'unittest.c', 'port_unittest.c']

paths = ['.', '../../eventos', '../../3rd/mdebug', '../../3rd/unity']

defines = ['main_function']
ccflags = []

env = Environment()
env.Append(CPPDEFINES = defines)
env.Append(CCCOMSTR = "CC $SOURCES")
env.Append(CPPPATH=paths)

obj = env.Object(src)
 
Return('obj')